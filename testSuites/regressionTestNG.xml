<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite  name="Regression Suite" parallel="classes" thread-count="2">
    <groups>
        <run>
            <include name="Regression"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="org.practice.utilsForTests.Listeners" />
    </listeners>
    <test verbose="2" preserve-order="true" name="Regression Test">
        <classes>
            <class name="org.practice.tests.TraversalPageTest"></class>
            <class name="org.practice.tests.LandingPageTest"></class>
        </classes>
    </test>
</suite>